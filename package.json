{
  "name": "typescript-action",
  "version": "2.0.1",
  "private": true,
  "description": "Setup yq",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freenet-actions/setup-yq.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "json-schema": ">=0.4.0",
    "tmpl": ">=1.0.5",
    "@actions/core": "1.8.2",
    "@actions/tool-cache": "2.0.1"
  },
  "devDependencies": {
    "@types/jest": "28.1.1",
    "@types/node": "17.0.41",
    "@typescript-eslint/parser": "5.27.1",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.17.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.5.3",
    "jest": "28.1.1",
    "jest-circus": "28.1.1",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  }
}
