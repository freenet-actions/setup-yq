{
  "name": "setup-yq",
  "version": "3.1.1",
  "private": true,
  "description": "Setup yq",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freenet-actions/setup-yq.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "json-schema": ">=0.4.0",
    "tmpl": ">=1.0.5",
    "@actions/core": "1.11.1",
    "@actions/tool-cache": "2.0.2"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "20.19.33",
    "@typescript-eslint/parser": "7.18.0",
    "@vercel/ncc": "0.38.4",
    "eslint": "8.57.1",
    "eslint-plugin-github": "5.1.8",
    "eslint-plugin-jest": "28.14.0",
    "jest": "28.1.3",
    "jest-circus": "29.7.0",
    "js-yaml": "4.1.1",
    "prettier": "3.8.1",
    "eslint-plugin-prettier": "5.5.5",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
