{
  "name": "typescript-action",
  "version": "2.0.8",
  "private": true,
  "description": "Setup yq",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freenet-actions/setup-yq.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "json-schema": ">=0.4.0",
    "tmpl": ">=1.0.5",
    "@actions/core": "1.10.0",
    "@actions/tool-cache": "2.0.1"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "20.5.9",
    "@typescript-eslint/parser": "6.5.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.48.0",
    "eslint-plugin-github": "4.10.0",
    "eslint-plugin-jest": "27.2.3",
    "jest": "28.1.3",
    "jest-circus": "29.6.4",
    "js-yaml": "4.1.0",
    "prettier": "3.0.3",
    "eslint-plugin-prettier": "5.0.0",
    "ts-jest": "28.0.8",
    "typescript": "5.2.2"
  }
}
